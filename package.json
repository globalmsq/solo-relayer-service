{
  "name": "msq-relayer-service",
  "version": "1.0.0",
  "private": true,
  "description": "MSQ Relayer Service - Meta Transaction Relay Infrastructure",
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "dev": "pnpm --filter @msq-relayer/relay-api run dev",
    "build": "turbo run build",
    "test": "turbo run test --filter='!@msq-relayer/integration-tests' && echo '' && echo '=== ALL TESTS PASSED ===' || (echo '' && echo '=== TESTS FAILED ===' && exit 1)",
    "test:e2e": "turbo run test:e2e",
    "test:cov": "turbo run test:cov --filter='!@msq-relayer/integration-tests'",
    "test:integration": "cd docker && docker compose run --rm integration-tests",
    "test:all": "pnpm test && pnpm test:e2e",
    "lint": "turbo run lint",
    "docker:up": "cd docker && docker compose up -d",
    "docker:down": "cd docker && docker compose down",
    "docker:build": "cd docker && docker compose up --build",
    "create-keystore": "node scripts/create-keystore.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/msq-relayer-service.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ethers": "^6.13.0",
    "turbo": "^2.7.5"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "tmp@<=0.2.3": ">=0.2.4",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "qs@<6.14.1": ">=6.14.1",
      "diff@<4.0.4": ">=4.0.4",
      "diff@>=5.0.0 <5.2.2": ">=5.2.2",
      "lodash@>=4.0.0 <=4.17.22": ">=4.17.23",
      "undici": "^6.23.0"
    }
  }
}
